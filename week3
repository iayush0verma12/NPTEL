#Assignment 1
l=[]
for i in L:
  if(i%5==0 and i%7==0):
    l.append(i)
  else:
    if(i%5==0):
      l.append(i)
    else:
      if(i%7==0):
        l.append(i)
l.sort()
print(l)


#Assignment 2
def rev(L,n):
  rev_sort=list(reversed(sorted(L)))
  l=[]
  for i in rev_sort[:n]:
    l.append(i)
  print(l,end="")

#Assignment 3
l=[]
for i in L:
  if(i%2!=0):
    l.append(i)
print(len(l),end="")
